  <ItemGroup>
    <PackageReference Include="Foxit.Android" Version="9.1.0" />
    <PackageReference Include="Foxit.Android.UIExtensions" Version="9.1.0" />
    <PackageReference Include="Xamarin.Google.Android.Material" Version="1.4.0" />
  </ItemGroup>
</Project>
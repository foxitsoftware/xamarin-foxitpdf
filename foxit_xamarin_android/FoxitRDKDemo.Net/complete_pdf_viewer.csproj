<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net7.0-android</TargetFramework>
    <SupportedOSPlatformVersion>21</SupportedOSPlatformVersion>
    <OutputType>Exe</OutputType>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationId>com.foxit.pdf.viewer</ApplicationId>
    <ApplicationVersion>1</ApplicationVersion>
    <ApplicationDisplayVersion>9.1.0</ApplicationDisplayVersion>
    <AndroidManifest>..\complete_pdf_viewer\Properties\AndroidManifest.xml</AndroidManifest>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\complete_pdf_viewer\App.cs" />
    <Compile Include="..\complete_pdf_viewer\MainActivity.cs" />
    <Compile Include="..\complete_pdf_viewer\MyApplication.cs" />
    <Compile Include="..\complete_pdf_viewer\PDFReaderActivity.cs" />
  </ItemGroup>

  <ItemGroup>
    <AndroidAsset Include="..\complete_pdf_viewer\Assets\complete_pdf_viewer_guide_android.pdf">
      <Link>Assets\complete_pdf_viewer_guide_android.pdf</Link>
    </AndroidAsset>
    <AndroidAsset Include="..\complete_pdf_viewer\Assets\Sample.pdf">
      <Link>Assets\Sample.pdf</Link>
    </AndroidAsset>
    <AndroidAsset Include="..\complete_pdf_viewer\Assets\uiextensions_config.json">
      <Link>Assets\uiextensions_config.json</Link>
    </AndroidAsset>
    <None Include="..\complete_pdf_viewer\Assets\AboutAssets.txt">
      <Link>Assets\AboutAssets.txt</Link>
    </None>
    <AndroidAsset Include="..\libs\rdk_key.txt">
      <Link>Assets\rdk_key.txt</Link>
    </AndroidAsset>
    <AndroidAsset Include="..\libs\rdk_sn.txt">
      <Link>Assets\rdk_sn.txt</Link>
    </AndroidAsset>
    <None Include="..\complete_pdf_viewer\Resources\AboutResources.txt">
      <Link>Resources\AboutResources.txt</Link>
    </None>
    <None Include="..\complete_pdf_viewer\Properties\AndroidManifest.xml">
        <Link>AndroidManifest.xml</Link>
    </None>
  </ItemGroup>

<ItemGroup>
    <AndroidResource Include="..\complete_pdf_viewer\Resources\layout\activity_main.xml">
      <SubType>Designer</SubType>
      <Link>Resources\layout\activity_main.xml</Link>
    </AndroidResource>
    <AndroidResource Include="..\complete_pdf_viewer\Resources\values\colors.xml">
